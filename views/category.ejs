<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Categories</title>
</head>
<body>
    <h1>Category</h1>
    <table border="1" cellspacing="0">
        <tr>
            <th> No </th>
            <th>Name</th>
            <th>Action</th>
        </tr>
        <% category.forEach ((el, index) => { %>
            <tr>
                <td><%= ++index %></td>
                <td><%= el.name %></td>
                <td>
                    <a href="/category/<%= el.id %>"><button>show medicine</button></a>
                </td>
            </tr>
        <% }) %>
    </table>
</body>
</html>